package com.sample

rule "山梨酸01.01"
/* ruleType:配方
   status:1
   standard:50l */
    when
        $m : Entity(types contains "巴氏杀菌乳、灭菌乳和调制乳", firedRules not contains "山梨酸01.01")
        $e : Entity(literal == "山梨酸", value (>0.0)) from $m.properties
    then
        $m.updateRiskyInfo("山梨酸01.01：山梨酸 小于 0.0 mg/kg", $e.getValue());
        $m.addRiskLevel(1.0);
        $m.setRisky(true);
        $m.addStandardId(50l);
        $m.addFiredRule("山梨酸01.01");
end

